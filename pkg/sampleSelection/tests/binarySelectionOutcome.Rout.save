> ## Leeman Lucas (and many others): binary outcome
> 
> set.seed(0)
> N <- 500
> rho <- 0.7
> library( "mvtnorm" )
> eps <- rmvnorm(N, c(0,0), matrix(c(1,rho,rho,1), 2, 2) )
> xs <- runif(N)
> ysX <- 3*xs + eps[,1]
> ys <- ysX > 0
> xo <- runif(N)
> yoX <- -1 + 2*xo + eps[,2]
> yo <- factor((yoX > 0)*(ys > 0))
>                            # binary outcome, only observable if ys>0
> print(table(ys, yo, exclude=NULL))
       yo
ys        0   1 <NA>
  FALSE  74   0    0
  TRUE  202 224    0
  <NA>    0   0    0
> library( "sampleSelection" )
